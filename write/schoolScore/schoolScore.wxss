page {
  height: 100%;
}

.container {
  padding: 0;
}

.flex {
  display: flex;
}


.hoverLogin {
  background: #009502 !important;
}

/*主体内容*/
.wrap {
  position: relative;
  width: 100%;
  height: 100%;
  background: #fafafa;
  box-sizing: border-box;
}


.wrap .choise {
  height: 42px;
  padding: 16rpx;
  padding-left: 28rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #1fba21;
  background: #fff;
}

