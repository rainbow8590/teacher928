@import "../../utils/font.wxss";
.searchW {
  width: 100%;
  min-height: 108rpx;
  padding: 30rpx 30rpx 30rpx 28rpx;
  border-bottom: 1px solid #1FB923;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.searchW .searchIf{
  display: flex;
  flex-wrap: wrap;
}

.searchW .active {
  background: #1FB923;
  color: #fff;
}

.searchW .menu {
  /* padding: 0rpx 0 37rpx 0; */
  padding:0;

}

.searchW .menu image {
  display: block;
  width: 49rpx;
  height: 28rpx;
}
.iconfont{
  font-size:46rpx;
  color:#1FB923;

}