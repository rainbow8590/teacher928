@import "../../utils/public.wxss";
page{
  height: 100%;
}
.container{
  padding:0 30rpx 40rpx 30rpx;
  height: 100%;
  overflow-y: scroll;
  /*overflow:hidden;*/
}
.flex{
    display: flex;
   
}
view{
  width: 100%;
  box-sizing: border-box;
  font-size: 24rpx;
  text-align: justify;
}
.title{
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 80rpx;
  padding-top: 40rpx;
}
.h1{
  width:100%;
  line-height: 70rpx;
  background: #1fba21;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  margin: 40rpx 0;
}
.table{
  width:100%;
  border: 1px solid #1fba21;
}
.th{
  width: 100%;
  border-bottom: 1px solid #1fba21;
  text-align: center;
  background: #eaffea;
}
.th .td{
  line-height: 70rpx;
  width: 120rpx;
  border-right: 1px solid #1fba21;
}
.th .td1{
    flex:1;
    border-right: none;
}
.tr{
  width: 100%;
  border-bottom: 1px solid #1fba21;
}
.tr1{
  border-bottom: none;
}
.tr .td{
  justify-content: center;
  align-items: center;
  text-align: center;
  width: 120rpx;
  border-right: 1px solid #1fba21;
}
.tr .td1{
  border-right: none;
  flex:1;
  padding: 10rpx 14rpx;
  text-align: left;
}

.h2{
  font-size: 28rpx;
  line-height: 70rpx;
  font-weight: bold;
}
.pl20{
  padding-top: 10rpx;
  padding-left: 20rpx;
  line-height: 50rpx;
  color: #4fd651;
  font-weight: bold;
}
.pl40{
  padding-left: 40rpx;
  padding-top: 10rpx;
}
.pl60{
  padding-left: 60rpx;
   padding-top: 10rpx;
}
.pl80{
  padding-left: 80rpx;
   padding-top: 10rpx;
}