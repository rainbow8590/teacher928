.yes{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,.5);
  z-index: 9999;
}
.yes .yes-wrap{
  width: 570rpx;
  height: 500rpx;
  border-radius: 10rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -285rpx;
  margin-top: -250rpx;
  color: #32343d;
}
.yes .yes-wrap1{
  height: 320rpx;
  margin-top: -160rpx;

}
.yes .yes-wrap .content{
  line-height: 230rpx;
  text-align: center;
  font-size: 26rpx;
  border-bottom: 1px solid #eee;
}
.yes .yes-wrap textarea{
  display: block;
  width: 90%;
  height: 400rpx;
  line-height: 28rpx;
  padding: 10rpx 5%;
  border-bottom: 1px solid #ccc;
  margin: 0 auto;
  white-space: wrap;
  font-size: 24rpx;
}
.yes .yes-wrap .btn {
  width: 100%;
  font-size: 26rpx;
}

.yes .yes-wrap .btn button{
  border: none;
  background: #fff;
  width: 50%;
  text-align: center;
  line-height: 80rpx;
}
.yes .yes-wrap .btn button.off{
  color:#ff4545; 
}
button {
  position:static;
  font-size:26rpx;
  overflow:hidden;
  margin-left: 0;
}
button::after {
  content:none;
}
.yes1 {
  height: 450rpx;
  bottom: 0;
  top: auto;
}
.yes1 .btns{
  width: 480rpx;
  height: 200rpx;
  overflow: hidden;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -240rpx;
  margin-top: -170rpx;
  color: #32343d;
}
.yes1 .btns button{
  width: 480rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
  margin-bottom: 40rpx;
}


.arrow-left{
    display: inline-block;
    width: 7px;
    height: 7px;
    border-top: 1px solid #c6c6c6;
    border-right: 1px solid #c6c6c6;
    transform: rotate(45deg);
    margin-left: 28rpx;
}
.fadein{
    animation:fadein .4s  ease both;
}
@keyframes fadein {
    0%{opacity:0;left:0 }
    100%{opacity:1;left:0 }
}
.fadeout{
    animation:fadeout .4s  ease both;
}
@keyframes fadeout {
    0%{opacity:1; left:0;}
    99%{opacity:0 ; left:0;}
    100%{opacity:0 ; left:-100%;}
}
button::after{
    border: none;
}