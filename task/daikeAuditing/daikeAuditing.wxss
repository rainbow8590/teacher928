@import "../../utils/public.wxss";
page{
  height: 100%;
}
.container{
  padding: 0;
  height: 100%;
  overflow:hidden;
}
.flex{
  display: flex;
}

/*主体内容*/
.wrap{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fafafa;
  box-sizing: border-box;
}
.searchW {
  width: 100%;
  height: 108rpx;
  padding: 30rpx 36rpx 30rpx 28rpx;
  border-bottom: 1px solid #1FB923;
  box-sizing: border-box;
  background: #fafafa;
  justify-content: space-between;
  align-items:center;
}
.searchW .active {
  background: #1FB923;
  color: #fff;
}
.searchW  .menu {
  display: block;
  width: 49rpx;
  height: 28rpx;
  padding: 10px 0 10px 15px;
}
