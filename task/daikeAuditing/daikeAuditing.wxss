scroll-view{
  height: 40px;
}
.wrap .tab{
  width: 100%;
}
.wrap .tab .tabTitle .titleItem{
  width: 50%;
  font-size: 14px;
  text-align: center;
  padding: 10px 0;
  border-bottom: 1px solid #1FB923;
  position: relative;
  display: inline-block;
  color: #979797;
  overflow-y: scroll;
}
.wrap .tab .tabTitle .titleActive{
  color: #32343d;
  background: #f1f1f1;
}
.wrap .tab .tabTitle .titleItem .arrow{
  border: 4px solid #1FB923;
  border-top-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  width: 0;
  height: 0;
  position: absolute;
  left: 50%;
  bottom: 0;
  margin-left: -4px;
}
.wrap .tab .tabBody{
  width: 200%;
  position: relative;
}
.wrap .tab .tabBodyWrap{
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.wrap .tab .tabBody .tabBodyItem{
  width: 50%;
  flex-direction: row;
  overflow-y:scroll;
}
.wrap .tab .tabBody .tabBodyItem .noNotes{
  text-align: center;
  line-height: 300rpx;
  font-size: 60rpx;
  color: #ccc;
}
.activeStyle{
  background: #1FBB1C;
  color: #fff !important;
}

.progressItem{
  width: 741rpx;
  height: 541rpx;
  background-image: url("data:image/png;base64,");
  background-size: 741rpx 541rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: relative;
}
.progressItem .progressonIcon{
  position: absolute;
  right: 23rpx;
  top: 40rpx;
  padding: 0 14rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #fff;
  background: #f8974a;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
}
.progressItem .progressonIcon.qingqiu{
  background: #f8974a;
}
.progressItem .progressonIcon.shenhe{
  background: #4799e3;
}
.progressItem .progressonIcon.daike{
  background: #18984e;
}
.progressItem .progressonIcon.chaoshi{
  background: #ff715b;
}

.progressItem .title{
  width: 100%;
  height: 128rpx;
  padding: 102rpx 0 0 0;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
}
.progressItem .title text{
  text-decoration: underline;
}
.progressItem .content{
  font-size: 24rpx;
  line-height: 65rpx;
  padding-top:15rpx;
  padding-bottom:12rpx;
}
.progressItem .contentItem{
   margin: 0 74rpx;
  border-bottom: 1px solid #ebebeb;
}
.progressItem .contentItem .left{
  width:174rpx;
  text-align: center;
  font-size: 22rpx;
  color: #888;
}
.progressItem .btn{
  position: absolute;
  right: 50rpx;
  bottom: 40rpx;
  color: #fcb856;
  height: 46rpx;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
}
.progressItem .btn .look{
  color: blue;
}

/*课节弹窗*/
.float1{
  font-size: 26rpx;
  line-height: 48rpx;
  color: #32343d;
}

.float1 .floatWrap{
  min-height: 560rpx;
  overflow: hidden;
  overflow-y: scroll;
}
.float1 label{
  display: block;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 20rpx;
  border-bottom: 1px solid #d9d9d9;
}
.float1 label text{
  display: inline-block;
  width: 500rpx;
}
.float1 radio .wx-radio-input{
   border-radius: 50%;
   width: 30rpx;
   height: 30rpx;
}
.float1 radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background-color: rgba(0,0,0,1);
}

.float2{
  text-align: center;
  font-size: 30rpx;
  line-height: 60rpx;
}
.float2 .floatWrap{
  padding: 0 20rpx;
}
.float2 .title{
  padding-top: 20rpx;
}
.float2 .now{
  text-align: left;
  color: #4fb2cf;
  text-align: center;
}
.float2 label{
  width: 50%;
  display:inline-block;
}
.float2 radio-group{
   padding-top: 50rpx;
}
.float2 radio .wx-radio-input{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
}
.float2 radio .wx-radio-input.wx-radio-input-checked{
  border: none;
  background-color: red;
}
.float2 radio .wx-radio-input.wx-radio-input-checked:before{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
   line-height: 40rpx;
   text-align: center;
   font-size:30rpx;
   color:#fff;
   background: red;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}

.leave{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,.5);
  z-index: 9999;
}
.leave .leave-wrap{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  /*background: #fff;*/
  position: absolute;
  left: 0;
  top: 0;
  color: #32343d;
}
.leave .leave-wrap image{
  display: block;
  width: 80%;
  height: 60%;
  margin: 0 auto;
  margin-bottom: 40rpx;
  padding-top: 40rpx;
}
.leave .leave-wrap .no-leave{
  font-size: 50rpx;
  text-align: center;
  line-height: 300rpx;
  color:#ff4545;
}
.leave .leave-wrap .content{
  line-height: 230rpx;
  text-align: center;
  font-size: 26rpx;
  border-bottom: 1px solid #eee;
}
.leave .leave-wrap .btn {
  width: 100%;
  font-size: 26rpx;
  justify-content: center;
}
.leave .leave-wrap .btn button{
  border: none;
  background: #fff;
  width: 40%;
  text-align: center;
  line-height: 80rpx;
  margin: 0 5%;
}
.leave .leave-wrap .btn button.off{
  color:#ff4545; 
}