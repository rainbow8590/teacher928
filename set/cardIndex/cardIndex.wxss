@import "../../utils/public.wxss";

page {
  height: 100%;
}

.container {
  padding: 0;
  justify-content: flex-start;
  color: #32343d;
  background: #fafafa;
}

.flex {
  display: flex;
}

/*搜索条件*/
.searchW {
  width: 100%;
  height: 108rpx;
  padding: 30rpx 0 30rpx 28rpx;
  border-bottom: 1px solid #1FB923;
  box-sizing: border-box;
  background: #fff;
}

.searchW .active {
  background: #1FB923;
  color: #fff;
}

.searchW .menu {
  padding: 16rpx 0 37rpx 300rpx;
}

.searchW .menu image {
  display: block;
  width: 49rpx;
  height: 28rpx;
}

