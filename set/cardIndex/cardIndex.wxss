.searchResult {
  width: 100%;
  height: 400px;
  padding-left: 28rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  background: #fff;
  overflow-y: scroll;
}

.searchResult .resultItem {
  width: 100%;
  height: 58px;
  line-height: 58px;
  font-size: 26rpx;
  border-bottom: 1px solid #ddd;
}

.searchResult .resultItem .dot {
  width: 10rpx;
  height: 10rpx;
  background: #b2b8bc;
  border-radius: 50%;
  margin: auto 0;
  margin-left: 10rpx;
  margin-right: 28rpx;
}



.canvasStyle{
  position: fixed;
  left: 0;
  bottom: 56px;
  z-index: 1000;
}