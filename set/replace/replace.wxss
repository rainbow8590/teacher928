.wrap .tabBodyItem{
  width: 50%;
  flex-direction: row;
  overflow-y:scroll;
}
.wrap .tabBodyItem .noNotes{
  text-align: center;
  line-height: 300rpx;
  font-size: 60rpx;
  color: #ccc;
}
.wrap .tabBodyItem .newsItem{
  width: 100%;
  height: 112rpx;
  font-size: 26rpx;
  line-height: 112rpx;
  border-bottom: 1px solid #f1f1f1;
  box-sizing: border-box;
  /*justify-content: space-between;*/
}
.wrap .tabBodyItem .time{
  width: 180rpx;
}
.wrap .tabBodyItem .next{
  width: 180rpx;
  
}
.wrap .tabBodyItem .className{
  flex: 1;
  
}
.wrap .tabBodyItem .newsItem .newTip{
  display: block;
  width: 25rpx;
  height: 25rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  background-image: url(data:image/png;base64,);
  background-size: 25rpx 25rpx;
}
.wrap .tabBodyItem .newsItem .dot{
  margin-left: 10rpx;
  margin-right: 10rpx;
  color: #b2b8bc;
  font-size: 12px;
  width: 25rpx;
  height: 25rpx;
  text-align: center;
}
.activeStyle{
  background: #1FBB1C;
  color: #fff !important;
}

.progressItem{
  width: 741rpx;
  height: 541rpx;
  background-image: url("data:image/png;base64,");
  background-size: 741rpx 541rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: relative;
}
.progressItem .progressonIcon{
  position: absolute;
  right: 23rpx;
  top: 40rpx;
  padding: 0 14rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #fff;
  background: #f8974a;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
}
.progressItem .progressonIcon.qingqiu{
  background: #f8974a;
}
.progressItem .progressonIcon.shenhe{
  background: #4799e3;
}
.progressItem .progressonIcon.daike{
  background: #18984e;
}
.progressItem .progressonIcon.chaoshi{
  background: #ff715b;
}

.progressItem .title{
  width: 100%;
  height: 128rpx;
  padding: 102rpx 0 0 0;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
}
.progressItem .title text{
  text-decoration: underline;
}
.progressItem .content{
  font-size: 24rpx;
  line-height: 65rpx;
  padding-top:15rpx;
  padding-bottom:12rpx;
}
.progressItem .contentItem{
   margin: 0 74rpx;
  border-bottom: 1px solid #ebebeb;
}
.progressItem .contentItem .left{
  width:174rpx;
  text-align: center;
  font-size: 22rpx;
  color: #888;
}
.progressItem .btn{
  position: absolute;
  right: 50rpx;
  bottom: 40rpx;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
}
.progressItem .btn button{
  background: #fcb856;
  width: 200rpx;
  height: 46rpx;
  border-radius: 23rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
  margin-top: 8rpx;
  margin-left: 550rpx;
}












.float {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .3);
  z-index: 10000;
 }
 
 .float .floatWrap {
  width: 80%;
  height: 560rpx;
  padding: 0 12rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40%;
  margin-top: -280rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
 }
 
 .float .floatWrap .arrows {
  position: relative;
  height: 67rpx;
 }
 
 .float .floatWrap .arrows image {
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -9rpx;
  margin-top: -5rpx;
  width: 18rpx;
  height: 11rpx;
 }
 
 .float .floatWrap .floatC {
  height: 426rpx;
  overflow-y: scroll;
  overflow-x: hidden;
 }
 
 .float .floatWrap .floatC .floatC-item {
  height: 97rpx;
  line-height: 97rpx;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
  color: #333;
 }
 .float1 .floatWrap .floatC .floatC-item label{
  display: block;
  width: 100%;
 }


/*课节弹窗*/
.float1{
  font-size: 26rpx;
  line-height: 48rpx;
  color: #32343d;
}

.float1 .floatWrap{
  min-height: 560rpx;
  overflow: hidden;
  overflow-y: scroll;
}
.float1 label{
  display: block;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 20rpx;
  border-bottom: 1px solid #d9d9d9;
}
.float1 label text{
  display: inline-block;
  width: 500rpx;
}
.float1 radio .wx-radio-input{
   border-radius: 50%;
   width: 30rpx;
   height: 30rpx;
}
.float1 radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background-color: rgba(0,0,0,1);
}

.float2{
  text-align: center;
  font-size: 30rpx;
  line-height: 48rpx;
}
.float2 .floatWrap{
  padding: 0 20rpx;
}
.float2 .title{
  font-size: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.float2 .now{
  width: 50%;
  background: #fff;
  text-align: left;
  color: #4fb2cf;
  text-align: center;
  margin: 0 auto;
  margin-top: 30rpx;
}
.float2 .ok{
  background: #4fb2cf;
  color: #fff;
  font-size: 34rpx;
  width: 80%;
  height: 90rpx;
  margin: 0 auto;
  margin-top: 40rpx;
}
.float2 label{
  width: 50%;
  display:inline-block;
}
.float2 radio-group{
   padding-top: 32rpx;
}
.float2 radio .wx-radio-input{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
}
.float2 radio .wx-radio-input.wx-radio-input-checked{
  border: 1px solid red!important;
  background: red!important;
}
.float2 radio .wx-radio-input.wx-radio-input-checked:before{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
   line-height: 40rpx;
   text-align: center;
   font-size:36rpx;
   color:#fff;
   border-color: red!important;
   background: red;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%,-50%) scale(1);
}