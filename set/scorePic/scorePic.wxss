}

/*搜索条件*/
 .searchW {
  width: 100%;
  height: 108rpx;
  padding: 30rpx 36rpx 30rpx 28rpx;
  border-bottom: 1px solid #1FB923;
  box-sizing: border-box;
  background: #fff;
  justify-content: space-between;
  align-items:center;
}

.searchW .active {
  background: #1FB923;
  color: #fff;
}


.searchW  .menu {
  display: block;
  width: 49rpx;
  height: 28rpx;
  padding: 10px 0 10px 15px;
} 

