.searchResult {
  width: 100%;
  height: 400px;
  padding-left: 28rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  background: #fff;
  overflow-y: scroll;
}

.searchResult .resultItem {
  width: 100%;
  height: 58px;
  line-height: 58px;
  font-size: 26rpx;
  border-bottom: 1px solid #ddd;
}

.searchResult .resultItem .dot {
  width: 10rpx;
  height: 10rpx;
  background: #b2b8bc;
  border-radius: 50%;
  margin: auto 0;
  margin-left: 10rpx;
  margin-right: 28rpx;
}

.PreviewImg {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1000;
}




.canvasStyle{
  position: fixed;
  left: 0;
  bottom: 56px;
  z-index: 1000;
}


.float {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .3);
  z-index: 10000;
 }
 
 .float .floatWrap {
  width: 80%;
  height: 390rpx;
  padding: 0 12rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40%;
  margin-top: -195rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  overflow: hidden;
 }
 
 .float .floatWrap .arrows {
  position: relative;
  height: 67rpx;
 }
 
 .float .floatWrap .arrows image {
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -9rpx;
  margin-top: -5rpx;
  width: 18rpx;
  height: 11rpx;
 }
 
 .float .floatWrap .floatC {
  height: 426rpx;
  overflow-y: scroll;
  overflow-x: hidden;
 }
 
 .float .floatWrap .floatC .floatC-item {
  height: 97rpx;
  line-height: 97rpx;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
  color: #333;
  padding-left: 40rpx;
  text-align: left;
 }
 .float1 .floatWrap .floatC .floatC-item label{
  display: block;
  width: 100%;
 }  


