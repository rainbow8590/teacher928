/*向左滑动*/
.goLeft {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .6);
  color: #fff;
  z-index: 10000;
}

.goLeft view {
  text-align: left;
  font-size: 18px;
  padding-top: 152px;
  padding-bottom: 100rpx;
}

.goLeft view image {
  width:90%;
  margin-left: 5%;
  display:block;

}

.goLeft button {
  position: absolute;
  right: 60rpx;
  top: 160rpx;
  background: rgba(0, 0, 0, .0);
  width: 90px;
  height: 30px;
  line-height: 30px;
  border: 1px solid #fff;
  font-size: 12px;
  color: #fff;
  margin:0 auto;
}


/*升班弹窗*/
.upFloat{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.54);
  z-index: 19999;
}
.upFloat .floatWrap{
  position: absolute;
  left: 50%;
  top: 200rpx;
  width: 580rpx;
  padding:10px;
  margin-left: -290rpx;
  border-radius: 5px;
  background: #fff;
  box-sizing: border-box;
}
.upFloat .floatWrap .fItem{
  width: 100%;
  margin:30rpx 0;
  font-size: 22rpx;
}
.upFloat .floatWrap .fItem .fItemTop,.upFloat .floatWrap .fItem .fItemBottom{
  width:100%;
  min-height:54rpx;
  border: 1px solid #ddd;
  text-align: center;
  line-height: 54rpx;
  justify-content: space-between;
}
.upFloat .floatWrap .fItem .fItemBottom{
  border-top: none;
}
.upFloat .floatWrap .fItem .fItemTop view{
  border-right: 1px solid #ddd;
}
.upFloat .floatWrap .fItem .fItemBottom view{
  border-right: 1px solid #ddd;
}
.upFloat .floatWrap .fItem .fItemTop .sCode{
  width:160rpx;
}
.upFloat .floatWrap .fItem .fItemTop .sName{
  flex:1;
}
.upFloat .floatWrap .fItem .fItemTop .sTeacher{
  width:130rpx;
  border-right: none;
}
.upFloat .floatWrap .fItem .fItemBottom .sTime{
  flex:1;
}
.upFloat .floatWrap .fItem .fItemBottom .sSchool{
  width:230rpx;
  border-right: none;
}

/*左滑提示*/
.goLeftTip{
  position: absolute;
  right:0;
  top: 0;
  z-index:9999;
  width: 50px;
  height: 100%;
  font-size: 12px;
  padding-top: 200px;
  text-align: center;
  color: #fff;
}
.goLeftTip image{
  width: 77rpx;
  height: 104rpx;
/*  opacity: .5;*/
}