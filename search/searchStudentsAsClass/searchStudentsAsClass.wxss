.wrap .search {
  width: 95%;
  height: 50px;
  font-size: 26rpx;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding-bottom: 10px;
  margin: 0 auto;
  color: #696969
}

.wrap .search text {
  padding: 5px 5px;
}

.wrap .table {
  overflow: hidden;
  background: #fff;
  font-size: 13px;
  position: relative;
}

.wrap .table .thead {
  position: relative;
  height: 60px;
  line-height: 60px;
  /*border-bottom: 1px solid #1fba21;*/
  text-align: center;
  z-index: 1000;
}

.wrap .table .tbody {
  position: relative;
  overflow: hidden;
  overflow-y: scroll;
  border-bottom-color: transparent;
  z-index: 999;
}

.wrap .table .thead .thead-left {
  position: relative;
  background: #fff;
  z-index: 99;
}


.wrap .table .scrollItem {
  border-bottom: 1px solid #f1f1f1;
  border-right: 1px solid #1fba21;
  margin-right: 6px;
  height: 60px;
  box-sizing: border-box;
  background: #fff;
}


.wrap .table .tbody .thead-left .leftItem {
  border-bottom: 1px solid #f1f1f1;
  line-height: 20px;
  padding: 10px 0;
  height: 60px;
  white-space: nowrap;
  /*overflow-y: hidden;*/
  background: #fff;
  box-sizing: border-box;
  line-height: 40px;
  position: relative;
}

.wrap .table .thead .thead-right {
  position: relative;
  background: #fff;
  z-index: 88;
}

.wrap .table  .rightWrap {
  position: absolute;
  left: 0;
  top: 0;
  box-sizing: border-box;
  overflow: hidden;
  background: #fff;
}

.wrap .table .thead .twrap {
  position: absolute;
  left: 0;
  top: 0;
}

