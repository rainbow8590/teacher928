@import "../../utils/public.wxss";
@import "../../utils/font.wxss";
.iconfont{
  font-size: 40rpx;
}
.icon-arrowback{
  font-size: 40rpx;
  padding: 0 20rpx;
  color: #ccc;
}
page{
  height: 100%;
}
.container{
  padding: 0;
  height: 100%;
  overflow:hidden;
}
.flex{
  display: flex;
}

/*主体内容*/
.wrap{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fafafa;
  box-sizing: border-box;
}

.wrap .tab{
  width: 100%;
}

.wrap .tab .tabBodyItem{
  width: 100%;
  flex-direction: row;
  overflow-y:scroll;
}
.activeStyle{
  background: #1FBB1C;
  color: #fff !important;
}

.progressItem{
  width: 741rpx;
  height: 487rpx;
  background-image: url("data:image/png;base64,");
  background-size: 741rpx 487rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: relative;
}
.progressItem .progressonIcon{
  position: absolute;
  right: 23rpx;
  top: 40rpx;
  padding: 0 14rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #fff;
  background: #f8974a;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
}
.progressItem .progressonIcon.qingqiu{
  background: #f8974a;
}
.progressItem .progressonIcon.shenhe{
  background: #4799e3;
}
.progressItem .progressonIcon.daike{
  background: #18984e;
}
.progressItem .progressonIcon.chaoshi{
  background: #ff715b;
}

.progressItem .title{
  width: 100%;
  height: 128rpx;
  padding: 102rpx 0 0 0;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
}
.progressItem .title text{
  text-decoration: underline;
}
.progressItem .content{
  font-size: 24rpx;
  line-height: 65rpx;
  padding-top:15rpx;
  padding-bottom:12rpx;
}
.progressItem .contentItem{
   margin: 0 74rpx;
  border-bottom: 1px solid #ebebeb;
}
.progressItem .contentItem .left{
  width:174rpx;
  text-align: center;
  font-size: 22rpx;
  color: #888;
}
.progressItem .btn{
  background: #fcb856;
  width: 150rpx;
  height: 46rpx;
  border-radius: 23rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
  margin-top: 8rpx;
  margin-left: 550rpx;
}




.btns{
  text-align: center;
}
.btns .status{
  padding-top: 200rpx;
}
.btns .status1{
  padding-top: 0;
}
.btns .status button{
  display: inline-block;
  width: 260rpx;
  height: 78rpx;
  line-height: 78rpx;
  background: #62c74c;
  border-radius: 10rpx;
  color: #fff;
  font-size: 28rpx;
  margin:0 30rpx;
}
.btns .status .evaluate{
  margin: 0 auto;
  width: 560rpx;
  font-size: 24rpx;
  padding-top: 20rpx;
  color: #888;
}
.btns .statu button{
  display: inline-block;
  width: 260rpx;
  height: 78rpx;
  line-height: 78rpx;
  background: #62c74c;
  border-radius: 10rpx;
  color: #fff;
  font-size: 28rpx;
  margin:0 30rpx;
}

.btns .status button.btn1{
  background: #d1d1d1;
}
.btns .status button.btn2{
  width: 560rpx;
}

.btns .statu button.btn2{
  width: 560rpx;
}

.complate{
  padding-top: 20rpx;
  font-size: 24rpx;
}
.teachers{
  padding-bottom: 100rpx;
  justify-content: center;
  align-items: center;
}
.teachers image{
  width: 13rpx;
  height: 26rpx;
  padding: 0 40rpx;
  padding-top: 76rpx;
}
.teachers .teacher{
  width: 300rpx;
  height: 180rpx;
  border-radius: 20rpx;
  box-shadow: 0 0 10px #aaa;
  flex-direction: column;
  justify-content: center;
}
.teachers .teacher .phone{
  font-size: 22rpx;
  color: #888;
}


.yes{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,.5);
}
.yes .yes-wrap{
  width: 570rpx;
  height: 500rpx;
  border-radius: 10rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -285rpx;
  margin-top: -250rpx;
  color: #32343d;
}

.yes .yes-wrap1{
  height: 320rpx;
  margin-top: -160rpx;
}

.yes .yes-wrap .content{
  line-height: 230rpx;
  text-align: center;
  font-size: 26rpx;
  border-bottom: 1px solid #eee;
}
.yes .yes-wrap textarea{
  display: block;
  width: 90%;
  height: 400rpx;
  line-height: 28rpx;
  padding: 10rpx 5%;
  border-bottom: 1px solid #ccc;
  margin: 0 auto;
  white-space: wrap;
  font-size: 24rpx;
}
.yes .yes-wrap .inner {
  line-height: 240rpx;
  font-size: 26rpx;
  text-align: center;
  border-bottom: 1px solid #ccc;
}
.yes .yes-wrap .btn {
  width: 100%;
  font-size: 26rpx;
}

.yes .yes-wrap .btn button{
  border: none;
  background: #fff;
  width: 50%;
  text-align: center;
  line-height: 80rpx;
}
.yes .yes-wrap .btn button.off{
  color:#ff4545; 
}
.yes .yes-wrap1 .btn-sure button{
  width: 100%;
}
button {
  position:static;
  font-size:26rpx;
  overflow:hidden;
  margin-left: 0;
}

button::after {
  content:none;
}
